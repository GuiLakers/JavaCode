package somacoluna;


public class SomaColuna {

 

    public static void main(String[] args) {

        

        int [][] x = {{2,4,3},
                     {5,1,0},
                     {1,2,7}};

        int sum = 0;

       

        for (int i=0; i<x.length; i++) {

            sum += x[i][0];

        }

        System.out.println(sum);

 

        
    }

   

}
